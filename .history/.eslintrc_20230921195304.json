{
    "env": {
        "browser": true,
        "es2021": true,
        "jest": true
    },
    "extends": ["react-app", "react-app/jest", "airbnb", "eslint-config-react-app", "plugin:import/errors",
    "plugin:import/warnings",
    "plugin:import/typescript"],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "react/jsx-filename-extension": [1, { "extensions": [".js", ".jsx"] }],
        "react/jsx-uses-react": ["off"],
        "react/react-in-jsx-scope": ["off"],
        "no-shadow": ["off"]
      }
}
